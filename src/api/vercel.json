{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/client/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/client/$1"
    },
    {
      "handle": "filesystem"
    }
  ]
}
